{
  "name": "amphion",
  "version": "0.1.12",
  "description": "roslibjs based web visualization library",
  "main": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapyuta-robotics/amphion.git"
  },
  "keywords": [
    "ros",
    "robotics",
    "riz",
    "javascript",
    "threejs"
  ],
  "author": "Rapyuta Robotics <support@rapyuta-robotics.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rapyuta-robotics/amphion/issues"
  },
  "dependencies": {
    "@juggle/resize-observer": "^2.2.1",
    "roslib": "^1.0.1",
    "stats-js": "^1.0.1",
    "three": "^0.107.0",
    "three-freeform-controls": "^0.0.6",
    "urdf-loader": "^0.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "examples": "webpack-dev-server --config webpack.config.js",
    "lint": "eslint src/**/*.js --fix",
    "prettier": "prettier src/**/* --write",
    "add": "git add .",
    "pre-commit": "npm-run-all lint prettier add",
    "dev": "rollup -c --watch",
    "start": "npm-run-all --parallel dev examples"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "babel-loader": "^8.0.6",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-restricted-globals": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "object.assign": "^4.1.0",
    "object.entries": "^1.1.0",
    "prettier": "^1.18.2",
    "prettier-sort-destructure": "0.0.4",
    "rollup": "^1.16.7",
    "rollup-plugin-local-resolve": "^1.0.7",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  }
}
